PROJECT_DIR=`pwd`

### --------------------------------------------------------------------
### Build project
### --------------------------------------------------------------------
default:
	
arm:
	@./build.sh arm
### --------------------------------------------------------------------
### Download project dependency
### --------------------------------------------------------------------	
depends:
	@./build.sh depends

### --------------------------------------------------------------------
### Clean build
### --------------------------------------------------------------------	
clean:
	@rm -rf $(PROJECT_DIR)/build

### --------------------------------------------------------------------
### Update all library
### --------------------------------------------------------------------
update:
	@./build.sh update
