PROJECT_DIR=`pwd`

### --------------------------------------------------------------------
### Build project
### --------------------------------------------------------------------
default:
	
arm:
	@./build.sh arm
### --------------------------------------------------------------------
### Download project dependency
### --------------------------------------------------------------------	
depends:
	@./build.sh depends

install:
	@./build.sh install

run:
	@./build.sh run

### --------------------------------------------------------------------
### Clean build
### --------------------------------------------------------------------	
clean:
	@rm -rf $(PROJECT_DIR)/build
